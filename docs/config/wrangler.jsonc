{
  "name": "v2-docs",
  "routes": [{ "pattern": "v2.mirascope.com/*", "zone_name": "mirascope.com" }],
  "compatibility_date": "2025-06-10",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "./dist",
    "not_found_handling": "single-page-application",
  },
  "build": {
    "command": "uv run docs-build",
    "cwd": "../../",
    "watch_dir": "../",
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1,
  },
}
