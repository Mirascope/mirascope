{
  "name": "@mirascope/cloud",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "start": "bun scripts/start.ts",
    "build": "vite build",
    "build:full": "bun run prebuild && bun run generate-og-images && bun run build && bun run optimize-images && bun run prerender && bun run postbuild",
    "prebuild": "bun run preprocess-content",
    "postbuild": "pagefind --site dist --output-path dist/_pagefind --root-selector '[data-pagefind-body=\"true\"]' --",
    "preview": "vite preview",
    "deploy": "bun run build && wrangler deploy",
    "version-upload": "wrangler versions upload",
    "serve": "bun scripts/serve-static.ts",
    "cf-typegen": "wrangler types",
    "db:start": "docker compose -f docker/compose.yml up -d",
    "db:stop": "docker compose -f docker/compose.yml down",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "generate:openapi": "bun run api/generate-openapi.ts",
    "sync": "bun run sync-docs && bun run generate-api-docs --source-path .build-cache/mirascope/python --package mirascope.llm --output content/docs/mirascope/v2/api --api-root /docs/mirascope/v2/api",
    "sync-docs": "bun run scripts/sync-docs.ts",
    "generate-api-docs": "bun run scripts/generate-api-docs.ts",
    "preprocess-content": "bun run scripts/preprocess-content.ts",
    "fetch-github-stats": "bun run --bun scripts/fetch-github-stats.ts",
    "enumerate-routes": "bun run scripts/enumerate-routes.ts",
    "generate-og-images": "bun scripts/generate-og-images.ts",
    "test-prerender-route": "bun run scripts/test-prerender.ts",
    "prerender": "bun run scripts/vite-prerender.ts",
    "validate-assets": "bun run scripts/validate-assets.ts",
    "convert-to-webp": "bun run scripts/convert-to-webp.ts",
    "optimize-images": "bun run scripts/optimize-images.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "bun x tsc --build ./tsconfig.json",
    "lint": "bun run typecheck && bun run lint:mdx && bun run lint:python && bun run lint:eslint && bun run format:check",
    "lint:mdx": "bun run scripts/validate-mdx.ts",
    "lint:python": "cd api2mdx && uv run pyright api2mdx && uv run ruff check api2mdx && uv run pytest && echo \"Lint successful\"",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix": "bun run format && bun run fix:python && bun run lint:eslint:fix",
    "fix:python": "cd api2mdx && uv run ruff format api2mdx && uv run ruff check --fix api2mdx && uv run pytest",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,mdx}": "prettier --write",
    "*.{ts,tsx}": [
      "bun scripts/check-ts.ts"
    ],
    "content/**/*.mdx": [
      "bun run lint:mdx"
    ],
    "api2mdx/**/*.py": "bun run lint:python",
    "public/assets/**/*.{png,jpg,jpeg}": [
      "bun -e \"console.error('‚ùå ERROR: PNG/JPG files are not allowed.\\nPlease convert to WebP using: bun run convert-to-webp'); process.exit(1);\""
    ]
  },
  "dependencies": {
    "@effect/platform": "^0.93.3",
    "@mdx-js/react": "^3.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@shikijs/transformers": "^3.14.0",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.139.3",
    "@tanstack/react-router-devtools": "^1.139.3",
    "@tanstack/react-start": "^1.139.3",
    "@tanstack/router-plugin": "^1.139.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.0",
    "effect": "^3.19.6",
    "effect-query": "^0.2.1",
    "js-tiktoken": "^1.0.21",
    "lucide-react": "^0.513.0",
    "mermaid": "^11.12.1",
    "next-mdx-remote": "^5.0.0",
    "oniguruma-to-es": "4.2.0",
    "postgres": "^3.4.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rehype-pretty-code": "^0.14.1",
    "remark-gfm": "^4.0.1",
    "resend": "^4.8.0",
    "shiki": "^3.14.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.9.0",
    "@cloudflare/workers-types": "^4.20241127.0",
    "@effect/vitest": "^0.27.0",
    "@eslint/js": "^9.37.0",
    "@happy-dom/global-registrator": "^17.6.3",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/builder-vite": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "latest",
    "@types/jsdom": "^21.1.7",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "bun-types": "^1.3.1",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "pagefind": "^1.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "puppeteer": "^24.27.0",
    "sharp": "^0.34.4",
    "storybook": "^8.6.14",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4",
    "wrangler": "^4.45.3"
  }
}
