{
  "name": "@mirascope/cloud",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:full": "vite build && bun run optimize-images",
    "optimize-images": "bun run scripts/optimize-images.ts",
    "preview": "vite preview",
    "deploy": "bun run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "db:start": "docker compose -f docker/compose.yml up -d",
    "db:stop": "docker compose -f docker/compose.yml down",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "generate:openapi": "bun run api/generate-openapi.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "bun x tsc --build ./tsconfig.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix"
  },
